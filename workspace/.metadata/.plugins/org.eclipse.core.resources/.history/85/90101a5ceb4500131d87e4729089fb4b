package letter;

public class FormalAddress
{
	private String fisrtname = "Default";
	private String lastname = "Default";
	private String address = "Default";
	private String city = "Default";
	
	public FormalAddress(	String newFirstName,
							String newLastName,
							String newAddress,
							String newCity)
	{
		fisrtname = "Default";
		lastname = "Default";
		address = "Default";
		city = "Default";
	}
}