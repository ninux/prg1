package search;

import java.util.Random;

public class Main
{
	public static void main(String[] args)
	{
		// create a random sequence
		Random rand = new Random();
		int[] seq = new int[10];
		
		
	}
	
	public int getBiggest(int[] sequence)
	{
		int biggest = 0;
		
		for(int i = 0; i < sequence.length; i++){
			if(sequence[i] > biggest){
				biggest = sequence[i];
			}
		}
		
		return biggest;
	}
}