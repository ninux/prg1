package star;

public class Main
{
	public static void main(String[] args)
	{
		NameGenerator ng = new NameGenerator();
		
		System.out.println(ng.generateStarWarsName("Nino", "Ninux", "Fooman", "Bartown"));
		
		String a = "ABCD";
		System.out.println(a);
		
		String b = a;
		System.out.println(b);
		
		b = a.substring(0, 2);
		System.out.println(b);
		
	}
}