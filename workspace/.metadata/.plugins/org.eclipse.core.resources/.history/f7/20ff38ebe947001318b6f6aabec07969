package maxSubSum;

import java.util.ArrayList;

public class Main
{
	public static void main(String[] args)
	{
		int[] mySeq = new int[]{5, -8, 3, 4, -5, 7, -2, -7, 3, 5};
		maxSubtotal(mySeq);
		
	}
	
	public static int maxSubtotal(int[] sequence){

		int max = 0;
		
		for(int i = 0; i < sequence.length; i++){
			int subtotal = 0;
			System.out.print("{");
			for(int a = 0; a <= i; a++){
				subtotal += sequence[a];
				System.out.print(sequence[a] + ", ");
			}
			System.out.print("} = " + subtotal);
			System.out.println("");
		}
		return 0;
	}
}