package calc;

public class Main
{
	public static void main(String[] args)
	{
		int i = 3;
		double d = 3.0;
		boolean isFull = true;
		boolean isHot = false;
		
		System.out.println("Resultat: " + (10/i + 2));
		System.out.println("Resultat: " + (7/i + 1.0f));
		System.out.println("Resultat: " + (7 + i + 1.0f));
	}
}