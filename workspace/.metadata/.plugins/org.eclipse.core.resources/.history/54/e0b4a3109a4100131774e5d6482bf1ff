package myiterator;

import java.util.Iterator;

class FloatArrar implements Iterator<Float>
{
	private float[] array = new float[]{1,2,3,4,5}; 
	private int i = 0;
	
	public boolean hasNext()
	{
		if(i >= array.length)
		{
			return false;
		}
		return true;
	}

	@Override
	public Float next() {
		// TODO Auto-generated method stub
		return array[i++];
	}

	@Override
	public void remove() {
		// TODO Auto-generated method stub
		
	}
	
	
}


public class Main
{
	public static void main(String[] args)
	{
		
	}
}